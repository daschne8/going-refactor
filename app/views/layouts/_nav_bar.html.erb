<ul>
  <% if request.path != root_path %>
    <li><%= link_to('Home', root_path) %></li>
  <% end %>
  <% if logged_in? %>
    <li><%= link_to('Logout', logout_url, method: :post) %></li>
  <% else %>
      <li><%= link_to'Login', login_url %></li>
      <li><%= link_to "Sign up", new_occupant_path %></li>
  <% end %>
</ul>
