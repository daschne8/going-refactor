<h1><%=@occupant.name%></h1>


<%= render 'tags/filter' %>

<h2>We Need:</h2>
<div class="filter">
  <h3 id="type-of-filter"></h3>
</div>

<div class="goods-container dragscroll">

</div>
<br>

<div id = "dialog-1" title = "Add Good">
  <%= render 'goods/form' %>
</div>
<button id = "opener">Add Good</button>
<script>
   $(function() {
      $( "#dialog-1" ).dialog({
         autoOpen: false,
         modal: true,
         draggable: false,
         resizeable: false
      });
      $( "#opener" ).click(function() {
         $( "#dialog-1" ).dialog( "open" );
      });
   });
</script>
